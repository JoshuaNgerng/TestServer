{
  "version": 2,
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "functions": {
    "api/*.js": {
        "memory": 3000,
        "maxDuration": 50
    }
  },
  "rewrites": [
    {"source": "(.*)", "destination": "/api"},
    {"source": "(.*)", "destination": "/api/upload"}
    ]
}
